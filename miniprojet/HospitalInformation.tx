    private void AddBlockButtonActionPerformedActionPerformed(java.awt.event.ActionEvent evt) {                                                              

     String blockName = blockNameInput.getText();
        int floorNumber = Integer.parseInt(BlockFloorNumber.getSelectedItem().toString());
        String speciality = BlockSpeciality.getText();
        
        HospitalBlock newBlock = new HospitalBlock(blockName, floorNumber, speciality);
        hospital.addBlock(newBlock);
        
        blockNameInput.setText("");
        BlockSpeciality.setText("");
        DisplayHospitalBlocks();
        

    }                                                             

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        String name = HospitalNameInput.getText();
        int maxPatients = Integer.parseInt(HospitalMaxPatientsNumber.getSelectedItem().toString());
        String state = HospitalLocationState.getText();
        
        hospital.setName(name);
        hospital.setMaxPatients(maxPatients);
        hospital.setState(state);
        
        HospitalRecordManager recordManager = new HospitalRecordManager(hospital);
        recordManager.setVisible(true);
        this.dispose();
           
    }                                        

