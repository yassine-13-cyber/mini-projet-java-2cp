    public void DisplayHospitalBlocks() {
        DefaultTableModel model = (DefaultTableModel) jTable1.getModel();
        model.setRowCount(0);

        if (hospital != null) {
            ArrayList<HospitalBlock> blockList = hospital.getBlocks();
            for (HospitalBlock block : blockList) {
                Object[] rowData = {
                    block.getBlockName(),
                    block.getFloorNumber(),
                    block.getSpeciality(),
                    "" // Placeholder for max patients if needed
                };
                model.addRow(rowData);
            }
        }
    }

